{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "chrome",
        "request": "launch",
        "name": "Launch Chrome and start app",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}/src",
        "preLaunchTask": "npm: start", 
        "sourceMapPathOverrides": {
          "webpack:///src/*": "${webRoot}/*"
        }
      }
    ]
  }
  